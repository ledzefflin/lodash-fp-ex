{
  "name": "lodash-fp-ex",
  "version": "1.1.26",
  "description": "lodash/fp extensions",
  "keywords": [
    "lodash",
    "lodash-extensions",
    "lodash-fp",
    "lodash/fp",
    "lodashFp",
    "fp"
  ],
  "homepage": "https://github.com/ledzefflin/lodash-fp-ex",
  "bugs": {
    "url": "https://github.com/ledzefflin/lodash-fp-ex/issues",
    "email": "ledzefflin@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ledzefflin/lodash-fp-ex"
  },
  "license": "MIT",
  "author": "ledzefflin <ledzefflin@gmail.com>",
  "module": "index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "/dist",
    "/lib",
    "index.js",
    "index.js.map"
  ],
  "scripts": {
    "start": "tsc-watch --onSuccess \"node dist/index.js\" ",
    "build": "yarn test && rollup -c",
    "b": "tsc",
    "clean": "rimraf ./node_modules && rimraf dist && rimraf index.js index.js.map",
    "test": "mocha --require @babel/register",
    "types": "tsc ./lib/index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "package:install": "pnpm i && sort-package-json",
    "u": "pnpm up --latest -i && pnpm run package:install",
    "re": "rimraf ./node_modules && rimraf pnpm-lock.yaml && rimraf package-lock.json && rimraf yarn.lock && pnpm i"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/plugin-transform-exponentiation-operator": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-url": "^7.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/chai": "^4.3.3",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/prettier": "2.7.0",
    "@types/rimraf": "^3.0.2",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "prettier": "npm:@btmills/prettier",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "lodash": "^4.17.21"
  }
}
